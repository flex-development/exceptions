  "bin": {
    "yw": "./tools/cli/cjs/yw.cjs"
  },
  "scripts": {
    "clean:build": "rimraf ./packages/*/(cjs|dist|esm5|esm2015|types) || true",
    "clean:modules": "rimraf ./node_modules ./packages/*/node_modules",
    "clean:yarn": "rimraf .yarn/cache .yarn/*.gz",
    "clean": "concurrently \"yarn:clean:*\"",
    "postinstall": "is-ci || husky-run install",
    "prepack": "yarn workspaces foreach -iv --topological-dev run prepack",
    "bootstrap": "NODE_OPTIONS='-r tsconfig-paths/register' yarn prepack",
    "check:dedupe": "yarn dedupe --check",
    "check:format": "prettier --check .",
    "check:install": "yarn dlx @yarnpkg/doctor .",
    "check:style": "eslint . --ext md,js,ts",
    "check:upgrades": "yarn upgrade-interactive",
    "fix:format": "prettier --write .",
    "fix:style": "yarn check:style --fix --cache",
    "test": "bash tools/scripts/jest.sh",
    "test:exceptions": "yw exceptions test",
    "build": "yarn workspaces foreach -iv --topological-dev run build",
    "build:exceptions": "yw exceptions build",
    "release:exceptions": "yw exceptions release"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
